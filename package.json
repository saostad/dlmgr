{
  "name": "dlmgr",
  "version": "1.1.3",
  "description": "Node.js Download Manager",
  "repository": {
    "type": "git",
    "url": "https://github.com/saostad/dlmgr.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "bin/setup",
  "scripts": {
    "format": "prettier --check \"src/**/*.ts\" --write",
    "lint": "eslint --ext \".ts,.js\" src --quiet",
    "test": "jest",
    "test:watch": "jest --watch",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "prepublishOnly": "npm run tsc",
    "prod": "node --unhandled-rejections=strict dist/app.js",
    "dev": "nodemon --watch dist --exec \"npm run prod\"",
    "dev:docker": "nodemon -L --watch dist --exec \"npm run prod\"",
    "start": "npm-run-all -p dev tsc:watch"
  },
  "keywords": [
    "node",
    "typescript",
    "downloader",
    "download manager",
    "manager",
    "files",
    "pause-resume"
  ],
  "author": "Saeid Ostad",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "1000"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.21",
    "@types/cli-progress": "^3.8.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/url-join": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "axios": "^0.20.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "cli-progress": "^3.8.2",
    "fast-node-logger": "^1.4.4",
    "url-join": "^4.0.1"
  }
}
