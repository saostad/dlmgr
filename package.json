{
  "name": "get-files",
  "version": "1.0.0",
  "description": "Node.js Download Manager",
  "repository": {
    "type": "git",
    "url": "https://github.com/saostad/get-files.git"
  },
  "main": "dist/app.js",
  "types": "dist/app.d.ts",
  "bin": "bin/setup",
  "scripts": {
    "format": "prettier --check \"src/**/*.ts\" --write",
    "lint": "eslint --ext \".ts,.js\" src --quiet",
    "test": "jest",
    "test:watch": "jest --watch",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "prepublishOnly": "npm run tsc",
    "prod": "node --unhandled-rejections=strict dist/app.js",
    "dev": "nodemon --watch dist --exec \"npm run prod\"",
    "dev:docker": "nodemon -L --watch dist --exec \"npm run prod\"",
    "start": "npm-run-all -p dev tsc:watch"
  },
  "keywords": [
    "node",
    "typescript",
    "downloader",
    "download manager",
    "manager"
  ],
  "author": "Saeid Ostad",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "1000"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.16",
    "@types/cli-progress": "^3.4.2",
    "@types/jest": "^24.9.1",
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "cli-progress": "^3.6.0",
    "fast-node-logger": "^1.3.0"
  }
}
